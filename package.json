{
  "name": "discord-bugbuilder",
  "version": "1.0.0",
  "description": "A discord game inspired by carcassonne",
  "repository": {
    "url": "https://github.com/Drachenfrucht1/discord-burgbuilder",
    "type": "git"
  },
  "main": "./src/index.js",
  "scripts": {
    "start:prod": "tsc && npx cross-env NODE_ENV=production node ./src/index.js",
    "start:dev": "tsc && npx cross-env NODE_ENV=development node ./src/index.js",
    "start": "npm run start:prod",
    "debug": "tsc && node --inspect ./src/index.js",
    "lint": "eslint src/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Drachenfrucht1",
  "license": "MIT",
  "dependencies": {
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "dotenv": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
